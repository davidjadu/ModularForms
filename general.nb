      "NumDivergences", ",", "fnum", ",", "qorder", ",", "fde", ",", 
       "fullfact", ",", "max", ",", "temp", ",", "NumberofTerms", ",", 
       "divertpart", ",", "diver", ",", "solDiv", ",", "\[IndentingNewLine]", 
       "res", ",", "Pnumi", ",", "Pde", ",", "guess", ",", "possibilities", 
       ",", "invariants", ",", "solun", ",", "resun", ",", "k", ",", "nt", 
       ",", "\[Epsilon]", ",", "FullSol"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Print", "[", "\"\<I am starting\>\"", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", "Now", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"tt", "=", 
       RowBox[{"Array", "[", 
        RowBox[{"t", ",", 
         RowBox[{"Rank", "[", "algebra", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ntnt", "=", 
       RowBox[{"Array", "[", 
        RowBox[{"nt", ",", 
         RowBox[{"Rank", "[", "algebra", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"NumDivergences", "=", 
       RowBox[{"Max", " ", "/@", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"FOrthogonalFundamentalCoweights", "[", "algebra", "]"}], "//",
           "Transpose"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fnum", "[", "qorder_", "]"}], ":=", 
       RowBox[{"(", 
        RowBox[{"Times", "@@", 
         SuperscriptBox["XX", 
          RowBox[{"NumDivergences", "+", "qorder"}]]}], ")"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"fde", "=", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"Times", "@@", 
           SuperscriptBox["tt", 
            RowBox[{"2", "##"}]]}], "&"}], "@", 
         RowBox[{"Total", "@", 
          RowBox[{"Exponents", "[", "algebra", "]"}]}]}], ")"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"fullfact", "[", "qorder_", "]"}], ":=", 
