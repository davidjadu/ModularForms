          ")"}], 
         RowBox[{"1", "-", "qorder"}]]]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"temp", "[", "qorder_", "]"}], ":=", 
       RowBox[{
        RowBox[{"temp", "[", "qorder", "]"}], "=", 
        RowBox[{
         RowBox[{"PowerExpand", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"fullfact", "[", "qorder", "]"}], 
             RowBox[{"Pol", "[", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"algebra", ",", "num", ",", "qorder"}], "}"}], ",", 
               RowBox[{"df", "+", "qorder"}], ",", 
               RowBox[{"Rank", "[", "algebra", "]"}]}], "]"}]}], "/.", 
            RowBox[{
             RowBox[{"y", "[", "k_", "]"}], "\[RuleDelayed]", " ", 
             RowBox[{"zInv", "[", 
              RowBox[{"algebra", ",", "k"}], "]"}]}]}], "/.", 
           RowBox[{"replacement", "[", "algebra", "]"}]}], "]"}], "//", 
         "Expand"}]}]}], ";", 
      RowBox[{
       RowBox[{"NumberofTerms", "[", "qorder_", "]"}], ":=", 
       RowBox[{
        RowBox[{"NumberofTerms", "[", "qorder", "]"}], "=", 
        RowBox[{
         RowBox[{"Pol", "[", 
          RowBox[{"var", ",", 
           RowBox[{"df", "+", "qorder"}], ",", 
           RowBox[{"Rank", "[", "algebra", "]"}]}], "]"}], "//", 
         "Length"}]}]}], " ", 
      RowBox[{"(*", 
       RowBox[{"Number", " ", "of", " ", "coefficients"}], "*)"}], ";", 
      RowBox[{
       RowBox[{"divertpart", "[", "qorder_", "]"}], ":=", 
       RowBox[{
        RowBox[{"divertpart", "[", "qorder", "]"}], "=", 
        RowBox[{
         RowBox[{"\[Epsilon]", "*", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"temp", "[", "qorder", "]"}], "/.", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"t", "[", "k_", "]"}], "^", "n_"}], "/;", 
               RowBox[{"n", "<", "0"}]}], "\[RuleDelayed]", " ", 
              RowBox[{
               SuperscriptBox[
                RowBox[{"nt", "[", "k", "]"}], 
                RowBox[{"-", "n"}]], 
               RowBox[{"\[Epsilon]", "^", 
                RowBox[{"(", 
                 RowBox[{"-", "n"}], ")"}]}]}]}]}], ",", 
            "\[IndentingNewLine]", "\[Epsilon]"}], "]"}]}], "//", 
         "Expand"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"max", "[", "qorder_", "]"}], ":=", 
       RowBox[{"Exponent", "[", 
        RowBox[{
         RowBox[{"divertpart", "[", "qorder", "]"}], ",", "\[Epsilon]"}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"diver", "[", 
        RowBox[{"qorder_", ",", "n_"}], "]"}], ":=", 
       RowBox[{
        RowBox[{"diver", "[", 
         RowBox[{"qorder", ",", "n"}], "]"}], "=", 
        RowBox[{
         RowBox[{"CoefficientRules", "[", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"n", "\[NotEqual]", "0"}], ",", 
             RowBox[{"Coefficient", "[", 
