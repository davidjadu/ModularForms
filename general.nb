               RowBox[{
                RowBox[{"divertpart", "[", "qorder", "]"}], "/.", 
                RowBox[{"solDiv", "[", 
                 RowBox[{"qorder", ",", 
                  RowBox[{"n", "-", "1"}]}], "]"}]}], ",", 
               SuperscriptBox["\[Epsilon]", "n"]}], "]"}], ",", 
             RowBox[{
              RowBox[{"divertpart", "[", "qorder", "]"}], "/.", 
              RowBox[{"\[Epsilon]", "\[Rule]", "0"}]}]}], "]"}], ",", 
           RowBox[{"Join", "[", 
            RowBox[{"ntnt", ",", "tt"}], "]"}]}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"All", ",", "2"}], "]"}], "]"}]}]}], ";", 
      RowBox[{
       RowBox[{"solDiv", "[", 
        RowBox[{"_", ",", 
         RowBox[{"-", "1"}]}], "]"}], "=", 
       RowBox[{"{", "}"}]}], ";", 
      RowBox[{
       RowBox[{"solDiv", "[", 
        RowBox[{"qorder_", ",", "n_"}], "]"}], ":=", 
       RowBox[{
        RowBox[{"solDiv", "[", 
         RowBox[{"qorder", ",", "n"}], "]"}], "=", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Join", "[", 
            RowBox[{"#", ",", 
             RowBox[{
              RowBox[{"solDiv", "[", 
               RowBox[{"qorder", ",", 
                RowBox[{"n", "-", "1"}]}], "]"}], "/.", "#"}]}], "]"}], "&"}],
           "@", 
          RowBox[{
           RowBox[{"Solve", "[", 
            RowBox[{
             RowBox[{"diver", "[", 
              RowBox[{"qorder", ",", "n"}], "]"}], "\[Equal]", "0"}], "]"}], 
           "[", 
           RowBox[{"[", "1", "]"}], "]"}]}], "//", "Expand"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
      "Print", "[", "\"\<I am starting to solve the divergent part\>\"", 
       "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{
       TemplateBox[{
         Cell[
          TextData["Off"]], "paclet:ref/Off"},
        "RefLink",
        BaseStyle->{"InlineFormula"}], "[", 
       RowBox[{"Solve", "::", "svars"}], "]"}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{
       "It", " ", "is", " ", "clear", " ", "that", " ", "it", " ", "acnnot", 
