            RowBox[{"Rank", "[", "algebra", "]"}], "+", "1"}]}], "}"}]}], 
        "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"terms", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{"{", 
        RowBox[{"Replace", "[", 
