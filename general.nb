      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"StringJoin", "[", 
           RowBox[{"\"\<At order q^\>\"", ",", 
