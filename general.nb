    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"gen", "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"1", "-", 
         RowBox[{"e4", " ", 
          SuperscriptBox["z", "4"]}]}]], 
