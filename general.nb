Cell[BoxData[
 RowBox[{
  RowBox[{"solve", "[", 
   RowBox[{"algebra_", ",", 
    RowBox[{"M_", ":", "M"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{"solve", "[", 
    RowBox[{"algebra", ",", "M"}], "]"}], "=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
