          SuperscriptBox["z", "6"]}]}]], 
       RowBox[{"Product", "[", 
        RowBox[{
         RowBox[{"1", "/", 
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{
             RowBox[{"\[Phi]", "[", 
