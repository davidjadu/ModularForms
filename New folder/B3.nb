     RowBox[{"rep", ",", "new", ",", "d", ",", "c", ",", "hold"}], "}"}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"rep", "=", 
      RowBox[{"y", "\[Rule]", 
       RowBox[{"-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
